{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./server/bin/www",
    "server": "nodemon --watch ./server --exec babel-node ./server/bin/www",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "layer": "cd dependencies/nodejs;npm install",
    "dev": "NODE_ENV=dev npm-run-all build server",
    "prod": "NODE_ENV=prod npm-run-all build server",
    "watch:dev": "nodemon",
    "test": "jest",
    "deploy": "sh sam-deploy.sh"
  },
  "nodemonConfig": {
    "exec": "npm run build",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "actions-on-google": "^2.12.0",
    "chance": "^1.1.4",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "babel-jest": "^25.1.0",
    "jest": "^25.1.0"
  }
}
